{
  "name": "yml-to-prisma",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/prettier": "^2.7.2",
    "esbuild": "^0.17.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsc && esbuild --bundle src/main.ts --outdir=dist --minify --platform=node --packages=external",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts api.json gen:openapi"
  },
  "dependencies": {
    "@nestjs/cli": "^9.2.0",
    "@prisma-fmt/cli": "^1.1.1",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "yaml": "^2.2.1"
  },
  "files": [
    "./dist"
  ],
  "bin": {
    "yppg": "./dist/main.js"
  }
}